<!--begin::Search-->
<div id="kt_header_search" class="header-search d-flex align-items-center w-lg-200px" data-kt-search-keypress="true" data-kt-search-min-length="2" data-kt-search-enter="enter" data-kt-search-layout="menu" data-kt-search-responsive="lg" data-kt-menu-trigger="auto" data-kt-menu-permanent="true" data-kt-menu-placement="bottom-start">
	<!--begin::Tablet and mobile search toggle-->
	<div data-kt-search-element="toggle" class="search-toggle-mobile d-flex d-lg-none align-items-center">
		<div class="d-flex">
			<%- theme.getIcon("magnifier", "fs-1 ") %>
		</div>
	</div>
	<!--end::Tablet and mobile search toggle-->
	<%- include(theme.getPartialPath("partials/search/partials/_form-inline.ejs")); %>
	<!--begin::Menu-->
	<div data-kt-search-element="content" class="menu menu-sub menu-sub-dropdown w-300px w-md-350px py-7 px-7 overflow-hidden">
		<!--begin::Wrapper-->
		<div data-kt-search-element="wrapper">
			<%- include(theme.getPartialPath("partials/search/partials/_results.ejs")); %>
			<%- include(theme.getPartialPath("partials/search/partials/_main.ejs")); %>
			<%- include(theme.getPartialPath("partials/search/partials/_empty.ejs")); %>
		</div>
		<!--end::Wrapper-->
		<%- include(theme.getPartialPath("partials/search/partials/_advanced-options.ejs")); %>
		<%- include(theme.getPartialPath("partials/search/partials/_preferences.ejs")); %>
	</div>
	<!--end::Menu-->
</div>
<!--end::Search-->